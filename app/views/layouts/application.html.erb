    <%= javascript_include_tag "assets/vendors/jvectormap/jquery-jvectormap.min.js" %>
    <%= javascript_include_tag "assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js" %>
    <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.js" %>

    <%#= javascript_include_tag "assets/js/off-canvas.js" %> <!-- CUSTOM JS -->
